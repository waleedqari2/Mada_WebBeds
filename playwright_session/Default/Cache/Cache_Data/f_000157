<script type="text/javascript" src="https://www.uhitravel.com/_laravel/public/js/introjs/intro.min_1652264697_.js"></script>
<link media="all" type="text/css" rel="stylesheet" href="https://www.uhitravel.com/_laravel/public/css/../js/introjs/introjs.min_1652264697_.css"/>
<script type="text/javascript">

    var guide = null;
    var productType = "accommodation";
    var isUmrahBranch = "1";
    var customerCod = "2115855";
    var cookieName = 'guide_offline_dismiss_'+customerCod;
    
    function setCookie(cname, cvalue, exdays) {
        if (typeof exdays !== 'undefined') {
            var d = new Date();
            d.setTime(d.getTime() + (exdays * 24 * 60 * 60 * 1000));
            var expires = "expires=" + d.toUTCString();
            document.cookie = cname + "=" + cvalue + ";" + expires + ";path=/ ";
        } else {  //session only cookie
            document.cookie = cname + "=" + cvalue + ";expires=0;path=/ ";
        }
    }


    function rememberCustomer() {
        if ($('#CustomerDuration').is(":checked")) {
            //if checked then needs to delete cookie so on next login it will display
            document.cookie = cookieName +'=; Path=/; Expires=Thu, 01 Jan 1970 00:00:01 GMT;';

            //skip for the current session
            Loader.show();
            $.ajax({
                url: 'https://www.uhitravel.com/interface/en/offlinePackage/skipIntro',
                method: 'POST',
                data: {
                    '_token': CSRF_TOKEN,
                    
                },
                success: function (response) {
                   if (response=="success") {
                    Loader.hide();
                       return;
                        
                    }

                }
            }).always(function () {
                Loader.hide();
            });
            
        } else {
            //if unchecked then set a cookie for 30 days so atleast 30 days user will not able to see msg again
            setCookie(cookieName, 'yes', 30);
        }
    }

    function initGuide() {
        if (guide !== null) {
            guide.exit();
        }
        guide = introJs();

        //check for umrah branch and product type as transfer
        if (productType == 'accommodation' && isUmrahBranch) {
            guide.setOptions({
                steps: [
                    {
                        element: $('[id="offlinePackagetab"]')[0],
                        intro: '<h3>Our Offline Packages are here!</h3><p>Click on Package and let us walk you through the new module.</p><br/><input id=CustomerDuration type=checkbox checked><label>Show on next Startup</label>'

                    },


                ],
                doneLabel: 'Skip for now',
                exitOnEsc: false,
                exitOnOverlayClick: false,
                showBullets: false,
            });
        } else if (productType == 'offlinepackage' && isUmrahBranch) {
            guide.setOptions({
                steps: [
                    {
                        element: $('input[id="startdate"]')[0],
                        intro: '<h3>Select Date</h3><p>Here you can filter results and select your preferred Arrival / Start date</p><br/><input id=CustomerDuration type=checkbox checked><label>Show on next Startup</label>',
                    },
                    {
                        element: $('input[id="searchtext"]')[0],
                        intro: '<h3>Looking for something in Particular?</h3><p>You can search here by keywords or package names</p><br/><input id=CustomerDuration type=checkbox checked><label>Show on next Startup</label>',
                    },
                    {
                        element: $('div.col-space:first-child')[0],
                        intro: '<h3>Package overview</h3><p>Here you can see a list of all the packages available, click on the Package now</p><br/><input id=CustomerDuration type=checkbox checked><label>Show on next Startup</label>',
                        hideNext: true
                    }
                ],
                skipLabel: 'Skip',
                doneLabel: 'Done',
                exitOnEsc: false,
                exitOnOverlayClick: false
            });
        } else {
            guide.setOptions({
                steps: [
                    {
                        element: $('[id="package-description"]')[0],
                        intro: '<h3>Package Details</h3><p>Here you will see a detailed summary of the package including detailed itinerary and package components. </p><br/><input id=CustomerDuration type=checkbox checked><label>Show on next Startup</label>',
                    },
                    {
                        element: $('[id="downloads-col"]')[0],
                        intro: '<h3>Download Section</h3><p>Here you can download the price list and Itinerary</p><br/><input id=CustomerDuration type=checkbox checked><label>Show on next Startup</label>',
                    },
                    {
                        element: $('[id="btn_submit"]')[0],
                        intro: '<h3>Send Booking Request</h3><p>After you have filled the lead passenger details and submitted the request, our team will process your request within 24h and send you the booking details and payment link. </p><br/><input id=CustomerDuration type=checkbox checked><label>Show on next Startup</label>',
                    },

                    {
                        element: $('[class="homepage_lang_container"]')[0],
                        intro: '<h3>My Bookings</h3><p>All your bookings will be shown in your Booking Management screen. Note that al bookings need to be paid to be confirmed. All unpaid bookings automatically </p><br/><input id=CustomerDuration type=checkbox checked><label>Show on next Startup</label>',
                    }
                ],
                skipLabel: 'Skip',
                doneLabel: 'Done',
                exitOnEsc: false,
                exitOnOverlayClick: false
            });
        }

        //done
        guide.oncomplete(rememberCustomer);

        //skip
        guide.onexit(rememberCustomer);

        guide.start();
    }

    // introjs part disabled from offline package
    //
    // $(window).load(function () {
    //     initGuide();
    // });

    


</script>


                

