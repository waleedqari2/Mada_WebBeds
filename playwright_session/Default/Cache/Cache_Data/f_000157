    <script type="text/javascript">
	    AlertError.heading = 'Attention!';
	    AlertInfo.heading = 'Info';

        	    $.fn.popover.Constructor.DEFAULTS.content = function() { var target = $(this).data('target'); if(target) { return $($(this).html()).html(); } return $(this).data('content'); };
	    $.fn.popover.Constructor.DEFAULTS.template = '<div class="popover tooltipContent lefttooltip" role="tooltip"><h4 class="popover-title"></h4><div class="popover-content"></div></div>';
	    $.fn.popover.Constructor.DEFAULTS.html = true;
	    $.fn.popover.Constructor.DEFAULTS.trigger = 'hover';
	    $.fn.popover.Constructor.DEFAULTS.placement = 'top';
	    $.fn.popover.Constructor.DEFAULTS.container = 'body';

	    $.fn.tooltip.Constructor.DEFAULTS.template = '<div class="tooltip tooltipContent" role="tooltip"><span class="tooltip-inner"></span></div>';
	    $.fn.tooltip.Constructor.DEFAULTS.placement = 'top';
	    $.fn.tooltip.Constructor.DEFAULTS.container = 'body';

		$.datepicker.setDefaults({
			firstDay: '0',
			onClose: function(dateText, inst)
			{
				$(this).attr("disabled", false);
			},
			beforeShow: function(input, inst)
			{
				$(this).attr("disabled", true);
			}
		});

        var loaderDefaultText = "";
        var loaderSearchText = "Searching for the best rates and availability";
        var validatorRegexp = /^[a-zA-Z0-9,\s\-\._]*$/;
        var validatorLatinError = "Please use only latin characters";

	    $(document).ready(function()
	    {
		    //Check to see if the window is top if not then display button
		    $(window).scroll(function()
		    {
			    if ($(this).scrollTop() > 100) {
				    $('#scrollToTop').fadeIn();
			    } else {
				    $('#scrollToTop').fadeOut();
			    }

			    if($(".main-container").height() < $(window).scrollTop()+ $(window).height())
			    {
				    var rows = $('.search-result_row.hidden:not(".notInFilterRange"):lt(5)');
					//rows.length == 0 ? ResultFilter.appendRemainingResults() : rows.removeClass('hidden').slideDown();
					rows.length == 0 ? $(document).trigger('appendRemainingResults.main') : rows.removeClass('hidden').slideDown();
			    }

		    });

		    //Click event to scroll to top
		    $('#scrollToTop').on('click', function()
		    {
			    $('html, body').animate({scrollTop : 0}, 800);
			    return false;
		    });

            $('#currencyMenu').find('a').click(function (e) {
			    e.preventDefault();
			    var dataHref = $(this).attr('href');
                  var modalParent = $('.header_top_panel');
                  dataHref += window.location.search;
                  /* check if in compare list we have something on it */
                  if (Compare.compareList.length > 0) {
                  ShowCurrencyDialog(dataHref);
                  } else {
			    location.href = dataHref;
                  }
            });
            $('#currentUserMenu').click(function (e) {
			    e.preventDefault();

                ShowUserChangeDialog($(this))
            });

            var ShowCurrencyDialog = function(dataHref){
                var dialogOptions = {
                    size: 'large',
                    className: 'default_language_popup',
                    message: trans('changeCurrency.modalContent'),
                    title: trans('changeCurrency.title'),
                    onEscape: function() {},
                        buttons: {
                            close: {
                                label: trans('lang.button.no'),
                                className: 'btn-link btn-small'
                            },
                            success: {
                            label: trans('lang.button.yes'),
                                className: 'search_btn',
                                callback: function() {
                                  Compare.removeLocalStorage('localStorageObject');
                                  location.href = dataHref;
                                  return true;
                                }
                            }

                        }
                 };

                var langDialog = bootbox.dialog(dialogOptions);
            };
            /*checkbox click function for umrah*/
               $('#uhspecialCode').hide();
                $('#uhsubPCC').hide();
                $('#subpccCheckbox').click(function () {
                    if ($(this).is(":checked")) {
                        $('#uhsubPCC').show();
                        $('#uhspecialCode').show();
                    } else {
                        $('[name="subPcc"]').val('');
                        $('[name="subPcc_text"]').val('');
                        $('#uhsubPCC').hide();
                        $('#uhspecialCode').hide();
                    }
            });

            /*checkbox click method ends*/

			/* contact us methd */
			$('.contact_us_link').on('click', function (e) {
				e.preventDefault();
				var dataHref = $(this).attr('href');
				contactUs(dataHref);
		    });

		    $('#langMenu').find('a').click(function(e){
			    e.preventDefault();
			    var dataHref = $(this).attr('href');
			    if(!disableLangChange){

				    ShowLangDialog($(this));
			    } else {
				    location.href = dataHref;
			    }
		    });

		    var ShowLangDialog = function(langElement){
			    var dataHref = langElement.attr('href');
			    var dataLang = langElement.data('lang');
			    var langText = langElement.text();

			    var dialogOptions = {
				    size: 'large',
				    className: 'default_language_popup',
				    message: trans('lang.message').replace(/%language%/, langText),
				    title: trans('lang.title'),
				    onEscape: function() {},
				    buttons: {
					    close: {
						    label: trans('lang.button.no'),
						    className: 'btn-link btn-small',
						    callback: function() {
							    if($("#remember_setting:checked").length > 0){
								    $.ajax({
									    url: BASE_CI_URL + '/set-language',
									    dataType: 'json',
									    type: "post",
									    async: true,
									    data: {_token: CSRF_TOKEN, remember:1}
								    }).done(function (response) {
									    if(dataHref !== ''){
										    location.href = dataHref;
									    }
								    }).always(function() {

								    });
							    } else if(dataHref !== ''){
								    location.href = dataHref;
							    }
							    return true;
						    }
					    },
					    success: {
						    label: trans('lang.button.yes'),
						    className: 'search_btn',
						    callback: function() {
							    //console.log("2");
							    $.ajax({
								    url: BASE_CI_URL + '/set-language',
								    dataType: 'json',
								    type: "post",
								    async: true,
								    data: {_token: CSRF_TOKEN, lang: dataLang, remember:$("#remember_setting:checked").length}
							    }).done(function (response) {
								    //console.log("response", response);
								    if(dataHref !== ''){
									    location.href = dataHref;
								    }
							    }).always(function() {

							    });
							    return true;
						    }
					    }

				    }
			    };

			    var langDialog = bootbox.dialog(dialogOptions);
			    langDialog.find('.modal-footer').prepend('<label style="float: left"><input type="checkbox" value=1 id="remember_setting"/> ' + trans('lang.checkbox') + '</label>');
		    }

		    var ShowUserChangeDialog = function(branchElement){
			    var dataUser = branchElement.data('id');
			    var branchName =  branchElement.data('name');

                var dialogOptions = {
                    size: 'normal',
                    className: 'default_language_popup',
                    message: "Would you like to remove %user% as default user?".replace(/%user%/, branchName),
                    title: "Change current user",
                    onEscape: function () {
                    },
                    buttons: {
                        close: {
                            label: "No",
                            className: 'btn-link btn-small',
                            callback: function () {
                            }
                        },
                        success: {
                            label: "Yes",
                            className: 'search_btn',
                            callback: function () {
                                Loader.show();
                                $.ajax({
                                    url: "https://www.uhitravel.com/interface/en/set-user",
                                    dataType: 'json',
                                    type: "post",
                                    async: true,
                                    data: {_token: CSRF_TOKEN, user_id: dataUser}
                                }).done(function (response) {
                                    AlertInfo.show(response.message);
                                    window.location.reload();
                                }).fail(function (err) {
                                    AlertError.show(err.responseJSON.message);
                                }).always(function () {
                                    Loader.hide();
                                });
                                return true;
                            }
                        }
                    }
			    };
			    bootbox.dialog(dialogOptions);
		    }

            // Loader content
            var loaderContent = $('#loaderContent');
            Loader.addContent(loaderContent);
            Loader.showCallback = function(){
                SHOW_DEFAULT_LOADER = true; //reset loader type
            }

            window.onbeforeunload = function(e)
			{
                if(SHOW_DEFAULT_LOADER)
				{
                    if (e.target.activeElement.id !== "sapInvoicePdf")
                    {
                        Loader.show(false, false);
                    }
				}
				else
				{
					// show loader on refresh
					//var sPageURL = window.location.search.substring(1);
					//var doSearch = sPageURL.toLowerCase().indexOf("dosearch");
					//if(doSearch != -1)
					if(document.location.href.indexOf('doSearch') != -1)
					{
						Loader.show(loaderSearchText, true);
					}
				}
            };
            window.onload = function () { Loader.hide(); }

            if(isOldIE() == 7){
                $('label.required, div.required').append('<span title="Required!" style="color:#f00;"> *</span>');
            }
	    });
        $(".slideto").pageslide({ direction: "left", modal: true });

        function redirectToB2C(that){            
            const dataToken = $(that).attr('data-token')
            Loader.show()
            $.ajax({
                url: BASE_CI_URL + '/umrah-b2c-login',
                dataType: 'json',
                type: "post",
                async: true,
                data: {_token: CSRF_TOKEN, _passcode: dataToken}
            }).done(function (response) {                
                if( response.success == true ){   
                    window.open(
                    MAQAM_B2C_DIRECT_LOGIN_URL+"?_token="+dataToken,
                    '_blank' // <- This is what makes it open in a new window.
                    );
                }
            }).always(function() {
                Loader.hide()
            });
        }
    </script>

    
        
