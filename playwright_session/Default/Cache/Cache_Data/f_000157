    <script type="text/javascript">
        $(document).ready(function () {
            var baseUrl = BASE_CI_URL;
            var currentPage = 'otherAccommodation'
            var maQamacc = '0'
            var maQamtrns = '0'
            var Otheramacc = '1'
            var Othertrans = '1'
            var currentLang = 'en'


            if ((currentPage == 'accommodation' || currentPage == 'otherAccommodation') && ( maQamacc ==false || Otheramacc ==false )){
                $("input#differentOccupancyToogle").attr("disabled", true);
                $(".uh-maqam-nonmaqam-toggle-switch .slider").addClass("disabled");
            }
            if ((currentPage == 'transfer' || currentPage == 'otherTransfer') && ( maQamtrns ==false || Othertrans ==false )){
                $("input#differentOccupancyToogle").attr("disabled", true);
                $(".uh-maqam-nonmaqam-toggle-switch .slider").addClass("disabled");
            }
            $('#differentOccupancyToogle').change(function () {
                if(currentLang=='en'){
                    if ($(this).prop('checked') == true){
                        if(currentPage == 'transfer'){
                            window.location = baseUrl + '/otherTransfer'
                        }else{
                            window.location = baseUrl + '/otherAccommodation'
                        }
                    }else{
                        if(currentPage == 'otherTransfer'){
                            window.location = baseUrl + '/transfer'
                        }else{
                            window.location = baseUrl + '/accommodation'
                        }
                    }
                }
                else{
                    if ($(this).prop('checked') == true){
                        if(currentPage == 'otherTransfer'){
                            window.location = baseUrl + '/transfer'
                        }else{
                            window.location = baseUrl + '/accommodation'
                        }

                    }else{
                        if(currentPage == 'transfer'){
                            window.location = baseUrl + '/otherTransfer'
                        }else{
                            window.location = baseUrl + '/otherAccommodation'
                        }
                    }
                }
            });
        });

    </script>
