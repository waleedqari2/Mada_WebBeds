    $(document).ready(function() {
        $('body').on('click','.hasBannerPopUp', function () {
            var $modal = $('#popup_banner');
            var imagePath = $(this).data('modalcontent');

            $modal.find('#popupBannerImg').attr('src', imagePath);
            $modal.modal();
        });
    });
