	jQuery(function($) {
		$(document).on('click', '#shoppingCart .dropdown-menu', function(event) { event.stopPropagation(); });
		
		$(document).on('click', '.header-new-pos-redirect .close-message', function() {
			$.get($(this).attr('href'));
			$(this).closest('.header-new-pos-redirect').hide();
			return false;
		});
	});
</script>

