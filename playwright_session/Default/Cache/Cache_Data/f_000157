opy.","walletConfirmation":"Booking Payment","serachRatechangetoGetroom":"As part of our commitment to provide you with the most current and accurate booking conditions, we have performed an additional check with our suppliers.<br \/>Here's what you need to know:","previousPrice":"<br \/><br \/><b>Previous Price: <\/b>","newPrice":"<br \/><b>New Price: <\/b>","serachGetroomLastContent":"<br \/><br \/><b>Understanding the Change:<\/b> We ensure the rates and policies displayed are up-to-date and not just cached information. Room rates and cancellation policies can change with market demand. This extra step reaffirms that what you book is what you get\u2014no surprises.","priceAlert":"New booking price alert"};
        Accommodation.Booking.bookingUrl = "https://www.uhitravel.com/interface/en/accommodation/passenger-details";
        Accommodation.Booking.additionalMandatoryServicesUrl = "https://www.uhitravel.com/interface/en/accommodation/additionalMandatoryServices";
        Accommodation.Booking.checkPINUrl = "https://www.uhitravel.com/interface/en/loyalty/check-pin";
        Accommodation.Booking.getNewROEUrl = "https://www.uhitravel.com/interface/en/loyalty/roe";
        Accommodation.Booking.ccCustomer = 0;
        Accommodation.Booking.init();

        
        $('[name="subPcc"]').parents('.field_holder').hide();
        $('[name="specialCode"]').parents('.field_holder').hide();
        $('[name="nonumrahacm"]').val(1);

		if(isUmrahCustomer && activeTab == 'accommodation')
		{
            Accommodation.Booking.ccCustomer = 0;
		}

		/**********UH 996 GSA logged in as branch***********/
		var branchId = '';
		var branchPaymentType = '';
		var authUserPaymentType = '551';


		if( isUmrahCustomer && branchId ){
			if( authUserPaymentType != '552' && branchPaymentType == '552' ){								
				Accommodation.Booking.ccCustomer = 1;				
			}
		}
		/**********UH 996 GSA logged in as branch***********/

    });

    //403 "Forbidden! You don`t have permission to access this area!" fix
    var searchFormValidity = 6900; //session validity minus 5 minutes
    function checkSearchFormValidity()
    {
        searchFormValidity -= 10;
        if(searchFormValidity <= 0) {
            window.location.reload(true); //force page reload from server
        }
        else {
            setTimeout(function(){
               checkSearchFormValidity();
            }, 10000);
        }
    }

    var _currentLang = 'en';

</script>

<script type="text/html" id="searchShowMoreDestinationScript">

    <div class="clear-form searchCitiesContainer">
        <div class="row">
            <div class="form-group col-sm-10">
                <label class="control-label col-sm-3" for="scCountrySelect">Country</label>
                <div class="col-sm-9" style="padding: 0px;">
                    <input class="form-control typeahead" id="scCountrySearch" placeholder="Start typing a Country" name="scCountrySearch" type="text">                </div>
                <input id="scCountrySelect" name="scCountrySelect" type="hidden" value="">                <input id="isSearchAllowCountry" name="isSearchAllowCountry" type="hidden" value="0">            </div>
        </div>
        <div class="row" id="areaContainer" style="display:none;">
            <div class="form-group col-sm-10">
                <label class="control-label col-sm-3" for="scAreaSelect">Area</label>
                <div class="col-sm-9" style="padding: 0px; width: 75%;">
                    <input class="form-control typeahead" id="scAreaSearch" placeholder="Start typing an Area" name="scAreaSearch" type="text">                </div>
                  <input id="scAreaSelect" name="scAreaSelect" type="hidden" value="">            </div>
        </div>

        <div class="row">
            <div class="form-group col-sm-10">
                <label class="control-label col-sm-3" for="scCitySelect">City</label>
                <div class="col-sm-9" style="padding: 0px;">
                    <input class="form-control typeahead" id="scCitySearch" placeholder="Start typing a City" name="scCitySearch" type="text">                </div>
                <input id="scCitySelect" name="scCitySelect" type="hidden" value="">            </div>
        </div>

        <div class="row" id="locationContainer"> 
            <div class="form-group col-sm-10">
                <label class="control-label col-sm-3" for="scLocationSelect">Geo Location</label>
                <div class="col-sm-9" style="padding: 0px; width: 75%;">
                    <input class="form-control typeahead" id="scLocationSearch" placeholder="Start typing a Location" name="scLocationSearch" type="text">                </div>
                <input id="scLocationSelect" name="scLocationSelect" type="hidden" value="">                <input id="PLocationTypeHidden" name="PLocationTypeHidden" type="hidden" value="">            </div>
        </div>

        <div class="row" id="hotelContainer">
            <div class="form-group col-sm-10">
                <label class="control-label col-sm-3" for="scHotelSelect">Hotel</label>
                <div class="col-sm-9" style="padding: 0px; width: 75%;">
                    <input class="form-control typeahead" id="scHotelSearch" placeholder="Start typing a Hotel" name="scHotelSearch" type="text">                </div>
                <input id="scHotelSelect" name="scHotelSelect" type="hidden" value="">            </div>
