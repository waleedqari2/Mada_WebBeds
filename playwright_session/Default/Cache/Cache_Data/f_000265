    //403 "Forbidden! You don`t have permission to access this area!" fix
    var searchFormValidity = 6900; //session validity minus 5 minutes
    function checkSearchFormValidity()
    {
        searchFormValidity -= 10;
        if(searchFormValidity <= 0) {
            window.location.reload(true); //force page reload from server
        }
        else {
            setTimeout(function(){
               checkSearchFormValidity();
            }, 10000);
        }
    }

    var _currentLang = 'en';

</script>

<script type="text/html" id="searchShowMoreDestinationScript">

    <div class="clear-form searchCitiesContainer">
        <div class="row">
            <div class="form-group col-sm-10">
                <label class="control-label col-sm-3" for="scCountrySelect">Country</label>
                <div class="col-sm-9" style="padding: 0px;">
                    <input class="form-control typeahead" id="scCountrySearch" placeholder="Start typing a Country" name="scCountrySearch" type="text">                </div>
                <input id="scCountrySelect" name="scCountrySelect" type="hidden" value="">                <input id="isSearchAllowCountry" name="isSearchAllowCountry" type="hidden" value="0">            </div>
        </div>
        <div class="row" id="areaContainer" style="display:none;">
            <div class="form-group col-sm-10">
                <label class="control-label col-sm-3" for="scAreaSelect">Area</label>
                <div class="col-sm-9" style="padding: 0px; width: 75%;">
                    <input class="form-control typeahead" id="scAreaSearch" placeholder="Start typing an Area" name="scAreaSearch" type="text">                </div>
                  <input id="scAreaSelect" name="scAreaSelect" type="hidden" value="">            </div>
        </div>

        <div class="row">
            <div class="form-group col-sm-10">
                <label class="control-label col-sm-3" for="scCitySelect">City</label>
