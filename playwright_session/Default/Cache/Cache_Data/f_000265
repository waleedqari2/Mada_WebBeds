        $(".slideto").pageslide({ direction: "left", modal: true });

        function redirectToB2C(that){            
            const dataToken = $(that).attr('data-token')
            Loader.show()
            $.ajax({
                url: BASE_CI_URL + '/umrah-b2c-login',
                dataType: 'json',
                type: "post",
                async: true,
                data: {_token: CSRF_TOKEN, _passcode: dataToken}
            }).done(function (response) {                
                if( response.success == true ){   
                    window.open(
                    MAQAM_B2C_DIRECT_LOGIN_URL+"?_token="+dataToken,
                    '_blank' // <- This is what makes it open in a new window.
                    );
                }
            }).always(function() {
                Loader.hide()
            });
        }
    </script>

    
        
