        <script type="text/javascript" src="https://www.uhitravel.com/_laravel/public/js/jstree/_lib/jquery.cookie_1652264697_.js"></script>
        <script>
            function responsiveHeader() {
                var x = document.getElementById("myLinks");
                if (x.style.display === "block") {
                    x.style.display = "none";
                } else {
                    x.style.display = "block";
                }
            }

            /**
             * Function to open close language dropdown in mobile
             * */
            function openLangDropDown() {
                let dropDown = $('#uh-lang-drop')
                if (dropDown.hasClass('uh-no-display')) dropDown.removeClass('uh-no-display')
                else dropDown.addClass('uh-no-display')
            }
            /**
             * Function to open close language dropdown in mobile
             * */
            function openAccDropDown() {
                let dropDown = $('#uh-acc-mob-drop')
                if (dropDown.hasClass('uh-no-display')) dropDown.removeClass('uh-no-display')
                else dropDown.addClass('uh-no-display')
            }

          
        </script>

        <script type="text/javascript">
        /* First Carousel */
        $('#landingCarousel').carousel({
            interval: false,
        });

        $('#landingCarousel2').carousel({
            interval: false,
        });

        /** Script for first carousel with two items */
        $('.carousel.carousel1 .item').each(function () {
            var next = $(this).next();
            var minPerSlide = 0;
            if (!next.length) {
                next = $(this).siblings(':first');
            }
            next.children(':first-child').clone().appendTo($(this));
            for (var i = 0; i < minPerSlide; i++) {
                next = next.next();
                if (!next.length) {
                    next = $(this).siblings(':first');
                }

                next.children(':first-child').clone().appendTo($(this));
            }
        });

        $('.carousel.carousel2 .item').each(function () {
            var next = $(this).next();
            var minPerSlide = 1;
            if (!next.length) {
                next = $(this).siblings(':first');
            }
            next.children(':first-child').clone().appendTo($(this));
            for (var i = 0; i < minPerSlide; i++) {
                next = next.next();
                if (!next.length) {
                    next = $(this).siblings(':first');
                }
                
                next.children(':first-child').clone().appendTo($(this));
            }
        });
    </script>


