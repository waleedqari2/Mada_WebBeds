				contactUs(dataHref);
		    });

		    $('#langMenu').find('a').click(function(e){
			    e.preventDefault();
			    var dataHref = $(this).attr('href');
			    if(!disableLangChange){

				    ShowLangDialog($(this));
			    } else {
				    location.href = dataHref;
			    }
		    });

		    var ShowLangDialog = function(langElement){
			    var dataHref = langElement.attr('href');
			    var dataLang = langElement.data('lang');
			    var langText = langElement.text();

			    var dialogOptions = {
				    size: 'large',
				    className: 'default_language_popup',
				    message: trans('lang.message').replace(/%language%/, langText),
				    title: trans('lang.title'),
				    onEscape: function() {},
				    buttons: {
					    close: {
						    label: trans('lang.button.no'),
						    className: 'btn-link btn-small',
						    callback: function() {
							    if($("#remember_setting:checked").length > 0){
								    $.ajax({
									    url: BASE_CI_URL + '/set-language',
									    dataType: 'json',
									    type: "post",
									    async: true,
									    data: {_token: CSRF_TOKEN, remember:1}
								    }).done(function (response) {
									    if(dataHref !== ''){
										    location.href = dataHref;
									    }
								    }).always(function() {

								    });
							    } else if(dataHref !== ''){
								    location.href = dataHref;
							    }
							    return true;
						    }
					    },
					    success: {
						    label: trans('lang.button.yes'),
						    className: 'search_btn',
						    callback: function() {
							    //console.log("2");
							    $.ajax({
								    url: BASE_CI_URL + '/set-language',
								    dataType: 'json',
								    type: "post",
								    async: true,
								    data: {_token: CSRF_TOKEN, lang: dataLang, remember:$("#remember_setting:checked").length}
							    }).done(function (response) {
								    //console.log("response", response);
								    if(dataHref !== ''){
									    location.href = dataHref;
								    }
							    }).always(function() {

							    });
							    return true;
						    }
					    }

				    }
			    };

			    var langDialog = bootbox.dialog(dialogOptions);
			    langDialog.find('.modal-footer').prepend('<label style="float: left"><input type="checkbox" value=1 id="remember_setting"/> ' + trans('lang.checkbox') + '</label>');
		    }

		    var ShowUserChangeDialog = function(branchElement){
			    var dataUser = branchElement.data('id');
			    var branchName =  branchElement.data('name');

                var dialogOptions = {
                    size: 'normal',
                    className: 'default_language_popup',
                    message: "Would you like to remove %user% as default user?".replace(/%user%/, branchName),
                    title: "Change current user",
                    onEscape: function () {
                    },
                    buttons: {
                        close: {
                            label: "No",
                            className: 'btn-link btn-small',
                            callback: function () {
                            }
                        },
                        success: {
                            label: "Yes",
                            className: 'search_btn',
                            callback: function () {
                                Loader.show();
                                $.ajax({
                                    url: "https://www.uhitravel.com/interface/en/set-user",
                                    dataType: 'json',
                                    type: "post",
                                    async: true,
                                    data: {_token: CSRF_TOKEN, user_id: dataUser}
                                }).done(function (response) {
                                    AlertInfo.show(response.message);
                                    window.location.reload();
                                }).fail(function (err) {
                                    AlertError.show(err.responseJSON.message);
                                }).always(function () {
                                    Loader.hide();
                                });
                                return true;
                            }
                        }
                    }
			    };
			    bootbox.dialog(dialogOptions);
		    }

            // Loader content
            var loaderContent = $('#loaderContent');
            Loader.addContent(loaderContent);
            Loader.showCallback = function(){
                SHOW_DEFAULT_LOADER = true; //reset loader type
            }

            window.onbeforeunload = function(e)
			{
                if(SHOW_DEFAULT_LOADER)
				{
                    if (e.target.activeElement.id !== "sapInvoicePdf")
                    {
                        Loader.show(false, false);
                    }
				}
				else
				{
					// show loader on refresh
					//var sPageURL = window.location.search.substring(1);
					//var doSearch = sPageURL.toLowerCase().indexOf("dosearch");
					//if(doSearch != -1)
					if(document.location.href.indexOf('doSearch') != -1)
					{
						Loader.show(loaderSearchText, true);
					}
				}
            };
            window.onload = function () { Loader.hide(); }

            if(isOldIE() == 7){
                $('label.required, div.required').append('<span title="Required!" style="color:#f00;"> *</span>');
            }
