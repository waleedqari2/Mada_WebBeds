    <style type="text/css">
       .searchCitiesContainer span.twitter-typeahead{
           width: 100%;
       }
    </style>
    <script type="text/javascript">

        $(document).ready(function ()
        {
            //get cookie data and set
            var $scCountrySearch = $('#scCountrySearch');
            $scCountrySearch.val(getModalCookie("countrySearch"));
            $('#scCountrySelect').val(getModalCookie("countrySelect"));

            var $scAreaSearch = $('#scAreaSearch');
            $scAreaSearch.val(getModalCookie("areaSearch"));
            $('#scAreaSelect').val(getModalCookie("areaSelect"));

            var $scCitySearch = $('#scCitySearch');
            $scCitySearch.val(getModalCookie("citySearch"));
            $('#scCitySelect').val(getModalCookie("citySelect"));

            var $scLocationSearch = $('#scLocationSearch');
            $scLocationSearch.val(getModalCookie("locationSearch"));
            $('#scLocationSelect').val(getModalCookie("locationSelect"));
            $('#PLocationTypeHidden').val(getModalCookie("locationType"));
            $('#isSearchAllowCountry').val(getModalCookie("isSearchAllowCountry"));

            //Call check and submit button to enable button accordingly
            checkAndDisableSubmit();

            $(".modal-dialog button").click(function () {

                if ($scCountrySearch.val() != undefined) {
                    setModalCookie("countrySearch", $scCountrySearch.val());
                }
                if ($('#scCountrySelect').val() != undefined) {
                    setModalCookie("countrySelect", $('#scCountrySelect').val());
                }
                if ($('#scAreaSearch').val() != undefined) {
                    setModalCookie("areaSearch", $('#scAreaSearch').val());
                }
                if ($('#scAreaSelect').val() != undefined) {
                    setModalCookie("areaSelect", $('#scAreaSelect').val());
                }
                if ($('#scCitySearch').val() != undefined) {
                    setModalCookie("citySearch", $('#scCitySearch').val());
                }
                if ($('#scCitySelect').val() != undefined) {
                    setModalCookie("citySelect", $('#scCitySelect').val());
                }
                if ($('#scLocationSearch').val() != undefined) {
                    setModalCookie("locationSearch", $('#scLocationSearch').val());
                }
                if ($('#scLocationSelect').val() != undefined) {
                    setModalCookie("locationSelect", $('#scLocationSelect').val());
                }
                if ($('#PLocationTypeHidden').val() != undefined) {
                    setModalCookie("locationType", $('#PLocationTypeHidden').val());
                }
                if ($('#isSearchAllowCountry').val() != undefined) {
                    setModalCookie("isSearchAllowCountry", $('#isSearchAllowCountry').val());
                }
                if ($("#areaContainer").is(":visible")) {
                    setModalCookie("areaVisible", 1);
                } else {
                    setModalCookie("areaVisible", 0);
                }
            });

            $scCountrySearch.on('keyup', function (e) {
                var keyCode = e.which || e.keyCode;
                if ((keyCode > 64 && keyCode < 91) || (keyCode > 96 && keyCode < 123) || keyCode === 8) {
                    $('#scCountrySelect').val('');
                    $('#isSearchAllowCountry').val('0');
                    $('#areaContainer').hide();
                    scClearSearchCitySelect();
                    scClearSearchAreaSelect();
                    scClearSearchLocationSelect();
                    scClearSearchHotelSelect();
                    checkAndDisableSubmit();
                }
            });

            $scAreaSearch.on('keyup', function (e) {
                var keyCode = e.which || e.keyCode;
                if ((keyCode > 64 && keyCode < 91) || (keyCode > 96 && keyCode < 123) || keyCode === 8) {
                    $('#scAreaSelect').val('');
                    scClearSearchCitySelect();
                    scClearSearchLocationSelect();
                    scClearSearchHotelSelect();
                    checkAndDisableSubmit();
                }
            });

            $scCitySearch.on('keyup', function (e) {
                var keyCode = e.which || e.keyCode;
                if ((keyCode > 64 && keyCode < 91) || (keyCode > 96 && keyCode < 123) || keyCode === 8) {
                    $('#scCitySelect').val('');
                    scClearSearchLocationSelect();
                    scClearSearchHotelSelect();
                    checkAndDisableSubmit();
                }
            });

            $scLocationSearch.on('keyup', function (e) {
                var keyCode = e.which || e.keyCode;
                if ((keyCode > 64 && keyCode < 91) || (keyCode > 96 && keyCode < 123) || keyCode === 8) {
                    $('#scLocationSelect').val('');
                    $('#hotelContainer').show();
                    $('#PLocationTypeHidden').val('');
                    scClearSearchHotelSelect();
                    checkAndDisableSubmit();
                }
            });

            $('#scHotelSearch').on('keyup', function (e) {
                var keyCode = e.which || e.keyCode;
                if ((keyCode > 64 && keyCode < 91) || (keyCode > 96 && keyCode < 123) || keyCode === 8) {
                    $('#scHotelSelect').val('');
                    checkAndDisableSubmit();
                }
            });

            var Country = new Bloodhound({
                datumTokenizer: function (d) {
                    return Bloodhound.tokenizers.whitespace(d.value);
                },
                queryTokenizer: Bloodhound.tokenizers.whitespace,
                remote: {
                    url: BASE_URL + '/' + ROUTE_FOLDER + '/autocomplete/search.php?query=%QUERY&l=%LANG&t=country',
                    ajax: {
                        beforeSend: function () {
                            $scCountrySearch.activity({
                                outside: true,
                                align: 'right',
                                width: 3,
                                length: 3
                            });
                        },
                        complete: function () {
                            $scCountrySearch.activity(false);
                        }
                    },
                    replace: function (url, query)
                    {
                        var backup = CI_PROXY_URL + '?c=Country&m=getCountryCodeNameArray&transformArr=0&query=%QUERY';
                        backup = backup.replace('%QUERY', encodeURIComponent(query));

                        return url.replace('%QUERY', encodeURIComponent(query))
                            .replace('%LANG', _currentLang)
                            + '&u=' + encodeURIComponent(backup);
                    }
                },
                rateLimitWait: 300,
                limit: 15,
                sorter: function (a, b) {
                    return a.index - b.index;
                }
            });

            Country.initialize();
            $scCountrySearch.typeahead({minLength: 1, highlight: true, hint: false, autoSelect: true}, {
                name: 'scCountrySelectComplete',
                displayKey: 'value',
                source: Country.ttAdapter(),
                templates: {
                    suggestion: function (ctx) {

                        ctx.value = ctx.value || ctx.Description;

                        var $details = ctx.value;
                        var $details_hint = '';

                        if (ctx.hint) {
                            $details_hint += '<br><small>' + ctx.hint.value + '</small>';
                        }
                        return '<p>' + $details + $details_hint + '</p>';
                    }
                }
            }).on('typeahead:selected typeahead:autocompleted', function (j, ctx) {
                $('#scCountrySelect').val(ctx.Id || ctx.id);
                $('#isSearchAllowCountry').val(ctx.AllowSearch);
                ajaxAreas();
                checkAndDisableSubmit();
            });

            var Area = new Bloodhound({
                datumTokenizer: function (d) {
                    return Bloodhound.tokenizers.whitespace(d.value);
                },
                queryTokenizer: Bloodhound.tokenizers.whitespace,
                remote: {
                    url: BASE_URL + '/' + ROUTE_FOLDER + '/autocomplete/search.php?query=%QUERY&l=%LANG&t=area&countryId=%COUNTRY',
                    ajax: {
                        beforeSend: function () {
                            $('#scAreaSearch').activity({
                                outside: true,
                                align: 'right',
                                width: 3,
                                length: 3
                            });
                        },
                        complete: function () {
                            $('#scAreaSearch').activity(false);
                        }
                    },
                    replace: function (url, query)
                    {
                        var countryID = $('#scCountrySelect').val();
                        var backup = CI_PROXY_URL + '?c=Area&m=getAreaCodeNameArray&transformArr=0&query=%QUERY&Country=%COUNTRY';
                        backup = backup.replace('%QUERY', encodeURIComponent(query))
                            .replace('%COUNTRY', countryID);

                        return url.replace('%QUERY', encodeURIComponent(query))
                                .replace('%LANG', _currentLang)
                                .replace('%COUNTRY', countryID)
                                + '&u=' + encodeURIComponent(backup);
                    }
                },
                rateLimitWait: 300,
                limit: 15,
                sorter: function (a, b) {
                    return a.index - b.index;
                }
            });

            Area.initialize();
            $scAreaSearch.typeahead({minLength: 1, highlight: true, hint: false, autoSelect: true}, {
                name: 'scAreaSelectComplete',
                displayKey: 'value',
                source: Area.ttAdapter(),
                templates: {
                    suggestion: function (ctx)
                    {
                        ctx.value = ctx.value || ctx.Description;
                        ctx.country = ctx.country || ctx.Country_Name;

                        var $details = ctx.value + (ctx.city && ctx.city.length > 0 ? ', ' + ctx.city : '') + (ctx.country && ctx.country.length > 0 ? ' - ' + ctx.country : '');
                        var $details_hint = '';

                        if (ctx.hint) {
                            $details_hint += '<br><small>' +
                                ctx.hint.value +
                                (ctx.hint.city.length > 0 ? ', ' + ctx.hint.city : '') +
                                (ctx.hint.country.length > 0 ? ', ' + ctx.hint.country : '') +
                                '</small>';
                        }

                        return '<p>' +
                            $details +
                            $details_hint +
                            '</p>';
                    }
                }
            }).on('typeahead:selected typeahead:autocompleted', function (j, ctx) {
                $('#scAreaSelect').val(ctx.Id || ctx.id);
                checkAndDisableSubmit();
            });

            var City = new Bloodhound({
                datumTokenizer: function (d) {
                    return Bloodhound.tokenizers.whitespace(d.value);
                },
                queryTokenizer: Bloodhound.tokenizers.whitespace,
                remote: {
                    url: BASE_URL + '/' + ROUTE_FOLDER + '/autocomplete/search.php?query=%QUERY&l=%LANG&t=city&countryId=%COUNTRY&areaId=%AREA',
                    ajax: {
                        beforeSend: function () {
                            $('#scCitySearch').activity({
                                outside: true,
                                align: 'right',
                                width: 3,
                                length: 3
                            });
                        },
                        complete: function () {
                            $('#scCitySearch').activity(false);
                        }
                    },
                    replace: function (url, query)
                    {
                        var countryID = $('#scCountrySelect').val();
                        var areaID = $('#scAreaSelect').val();

                        var backup = CI_PROXY_URL + '?c=City&m=getCityCodeNameArray&transformArr=0&withActiveProduct=1&withProduct=1&query=%QUERY&Country=%COUNTRY&Area=%AREA';
                        backup = backup.replace('%QUERY', encodeURIComponent(query))
                            .replace('%COUNTRY', countryID)
                            .replace('%AREA', areaID);

                        return url.replace('%QUERY', encodeURIComponent(query))
                                .replace('%LANG', _currentLang)
                                .replace('%COUNTRY', countryID)
                                .replace('%AREA', areaID)
                                + '&u=' + encodeURIComponent(backup);
                    }
                },
                rateLimitWait: 300,
                limit: 15,
                sorter: function (a, b) {
                    return a.index - b.index;
                }
            });

            City.initialize();
            $scCitySearch.typeahead({minLength: 1, highlight: true, hint: false, autoSelect: true}, {
                name: 'scCitySelectComplete',
                displayKey: 'value',
                source: City.ttAdapter(),
                templates: {
                    suggestion: function (ctx) {

                        ctx.value = ctx.value || ctx.text;

                        var $details = ctx.value + (ctx.country && ctx.country.length > 0 ? ' - ' + ctx.country : '');
                        var $details_hint = '';

                        if (ctx.hint) {
                            $details_hint += '<br><small>' +
                                ctx.hint.value +
                                (ctx.hint.country.length > 0 ? ', ' + ctx.hint.country : '') +
                                '</small>';
                        }

                        return '<p>' +
                            $details +
                            $details_hint +
                            '</p>';
                    }
                }
            }).on('typeahead:selected typeahead:autocompleted', function (j, ctx) {
                $('#scCitySelect').val(ctx.Id || ctx.id);
                checkAndDisableSubmit();
            });

            var Location = new Bloodhound({
                datumTokenizer: function (d) {
                    return Bloodhound.tokenizers.whitespace(d.value);
                },
                queryTokenizer: Bloodhound.tokenizers.whitespace,
                remote: {
                    url: BASE_URL + '/' + ROUTE_FOLDER + '/autocomplete/search.php?query=%QUERY&l=%LANG&t=geoLocation&countryId=%COUNTRY&areaId=%AREA&cityId=%CITY',
                    ajax: {
                        beforeSend: function () {
                            $('#scLocationSearch').activity({
                                outside: true,
                                align: 'right',
                                width: 3,
                                length: 3
                            });
                        },
                        complete: function () {
                            $('#scLocationSearch').activity(false);
                        }
                    },
                    replace: function (url, query)
                    {
                        var countryID = $('#scCountrySelect').val();
                        var areaID = $('#scAreaSelect').val();
                        var cityID = $('#scCitySelect').val();

                        var backup = CI_PROXY_URL + '?c=GeoLocation&m=getGeoLocationCodeNameArray&transformArr=0&query=%QUERY&Country=%COUNTRY&Area=%AREA&City=%CITY';
                        backup = backup.replace('%QUERY', encodeURIComponent(query))
                            .replace('%COUNTRY', countryID)
                            .replace('%AREA', areaID)
                            .replace('%CITY', cityID);

                        return url.replace('%QUERY', encodeURIComponent(query))
                                .replace('%LANG', _currentLang)
                                .replace('%COUNTRY', countryID)
                                .replace('%AREA', areaID)
                                .replace('%CITY', cityID)
                                + '&u=' + encodeURIComponent(backup)
                            ;
                    }
                },
                rateLimitWait: 300,
                limit: 15,
                sorter: function (a, b) {
                    return a.index - b.index;
                }
            });

            Location.initialize();
            $scLocationSearch.typeahead({minLength: 1, highlight: true, hint: false, autoSelect: true}, {
                name: 'scLocationSelectComplete',
                displayKey: 'value',
                source: Location.ttAdapter(),
                templates: {
                    suggestion: function (ctx) {
                        ctx.value = ctx.value || ctx.Description;
                        ctx.city = ctx.city || ctx.CityName;
                        ctx.country = ctx.country || ctx.CountryName;

                        var $details = ctx.value + (ctx.city && ctx.city.length > 0 ? ', ' + ctx.city : '') + (ctx.country && ctx.country.length > 0 ? ' - ' + ctx.country : '');
                        var $details_hint = '';

                        if (ctx.hint) {
                            $details_hint += '<br><small>' +
                                ctx.hint.value +
                                (ctx.hint.city.length > 0 ? ', ' + ctx.hint.city : '') +
                                (ctx.hint.country.length > 0 ? ', ' + ctx.hint.country : '') +
                                '</small>';
                        }

                        return '<p>' +
                            $details +
                            $details_hint +
                            '</p>';
                    }
                }
            }).on('typeahead:selected typeahead:autocompleted', function (j, ctx) {
                $('#scLocationSelect').val(ctx.Id || ctx.id);
                $('#PLocationTypeHidden').val(ctx.locationType || ctx.Type);
                Loader.show();
                checkAndDisableSubmit();
                Loader.hide();
            });

            var Hotel = new Bloodhound({
                datumTokenizer: function (d) {
                    return Bloodhound.tokenizers.whitespace(d.value);
                },
                queryTokenizer: Bloodhound.tokenizers.whitespace,
                remote: {
                    url: BASE_URL + '/' + ROUTE_FOLDER + '/autocomplete/search.php?query=%QUERY&l=%LANG&t=hotel&countryId=%COUNTRY&areaId=%AREA&cityId=%CITY&locationId=%LOCATION',
                    ajax: {
                        beforeSend: function () {
                            $('#scHotelSearch').activity({
                                outside: true,
                                align: 'right',
                                width: 3,
                                length: 3
                            });
                        },
                        complete: function () {
                            $('#scHotelSearch').activity(false);
                        }
                    },
                    replace: function (url, query)
                    {
                        var countryID = $('#scCountrySelect').val();
                        var areaID = $('#scAreaSelect').val();
                        var cityID = $('#scCitySelect').val();
                        var locationID = '';
                        if ($("#locationContainer").is(":visible")) {
                            locationID = $('#scLocationSelect').val();
                        }

                        var backup =  CI_PROXY_URL + '?c=Hotel&m=getHotelCodeNameArray&query=%QUERY&Country=%COUNTRY&Area=%AREA&City=%CITY&Location=%LOCATION';
                        backup = backup.replace('%QUERY', encodeURIComponent(query))
                            .replace('%COUNTRY', countryID)
                            .replace('%AREA', areaID)
                            .replace('%CITY', cityID)
                            .replace('%LOCATION', locationID)
                        ;

                        return url.replace('%QUERY', encodeURIComponent(query))
                                .replace('%LANG', _currentLang)
                                .replace('%COUNTRY', countryID)
                                .replace('%AREA', areaID)
                                .replace('%CITY', cityID)
                                .replace('%LOCATION', locationID)
                                + '&u=' + encodeURIComponent(backup)
                            ;
                    }
                },
                rateLimitWait: 300,
                limit: 15
            });

            Hotel.initialize();
            $('#scHotelSearch').typeahead({minLength: 1, highlight: true, hint: false, autoSelect: true}, {
                name: 'scHotelSelectComplete',
                displayKey: 'value',
                source: Hotel.ttAdapter(),
                templates: {
                    suggestion: function (ctx) {
                        ctx.value = ctx.value || ctx.Name;
                        ctx.city = ctx.city || ctx.CityName;
                        ctx.country = ctx.country || ctx.CountryName;

                        var $details = ctx.value + (ctx.city && ctx.city.length > 0 ? ', ' + ctx.city : '') + (ctx.country && ctx.country.length > 0 ? ' - ' + ctx.country : '');
                        var $details_hint = '';

                        if (ctx.hint) {
                            $details_hint += '<br><small>' +
                                ctx.hint.value +
                                (ctx.hint.city.length > 0 ? ', ' + ctx.hint.city : '') +
                                (ctx.hint.country.length > 0 ? ', ' + ctx.hint.country : '') +
                                '</small>';
                        }

                        return '<p>' +
                            $details +
                            $details_hint +
                            '</p>';
                    }
                }
            }).on('typeahead:selected typeahead:autocompleted', function (j, ctx) {
                $('#scHotelSelect').val(ctx.Id || ctx.CodHotel);
                checkAndDisableSubmit();
            });

            function ajaxAreas() {
                var selectedCountry = $('#scCountrySelect').val();
                if (selectedCountry == '') {
                    return false;
                }
                Loader.show();
                $.ajax({
                    url: CI_PROXY_URL,
                    data: {
                        'c': 'Area',
                        'm': 'getAreaCodeNameArray',
                        'static': 'true',
                        'params': [new Array(selectedCountry), 0, null, 0, 0, 1]
                    },
                    dataType: 'json',
                    type: 'get',
                    async: true,
                    statusCode: {
                        302: function () {
                            window.location.href = BASE_CI_URL + '/login';
                        }
                    }
                }).done(function (result) {
                    if (Array.isArray(result) && result.length) {
                        $('#areaContainer').show();
                    } else {
                        $('#areaContainer').hide();
                    }
                }).always(function () {
                    Loader.hide();
                });
            }

            function scClearSearchCitySelect() {
                $('#scCitySelect').val('');
                $('#scCitySearch').val('');
                $('#scCitySearch').typeahead('val', '');
            }

            function scClearSearchAreaSelect() {
                $('#scAreaSelect').val('');
                $('#scAreaSearch').val('');
                $('#scAreaSearch').typeahead('val', '');
            }

            function scClearSearchLocationSelect() {
                $('#scLocationSearch').val('');
                $('#scLocationSelect').val('');
                $('#PLocationTypeHidden').val('');
                $('#scLocationSearch').typeahead('val', '');
            }

            function scClearSearchHotelSelect() {
                $('#scHotelSelect').val('');
                $('#scHotelSearch').val('');
                $('#scHotelSelect').typeahead('val', '');
            }

        });

    </script>

</script>
