	    $(document).ready(function()
	    {
		    //Check to see if the window is top if not then display button
		    $(window).scroll(function()
		    {
			    if ($(this).scrollTop() > 100) {
				    $('#scrollToTop').fadeIn();
			    } else {
				    $('#scrollToTop').fadeOut();
			    }

			    if($(".main-container").height() < $(window).scrollTop()+ $(window).height())
			    {
				    var rows = $('.search-result_row.hidden:not(".notInFilterRange"):lt(5)');
					//rows.length == 0 ? ResultFilter.appendRemainingResults() : rows.removeClass('hidden').slideDown();
					rows.length == 0 ? $(document).trigger('appendRemainingResults.main') : rows.removeClass('hidden').slideDown();
			    }

		    });

		    //Click event to scroll to top
		    $('#scrollToTop').on('click', function()
		    {
			    $('html, body').animate({scrollTop : 0}, 800);
			    return false;
