            $('#currencyMenu').find('a').click(function (e) {
			    e.preventDefault();
			    var dataHref = $(this).attr('href');
                  var modalParent = $('.header_top_panel');
                  dataHref += window.location.search;
                  /* check if in compare list we have something on it */
