<script type="text/javascript">
var gridster      = null;
$(document).ready(function ()
{
    initGridster();
});

 var resizeId;
 $(window).resize(function() {
    clearTimeout(resizeId);
    resizeId = setTimeout(doneResizing, 10);
});
function doneResizing() {
    gridster.destroy();
    initGridster();
}

function initGridster()
 {
     var parentWidth = $('.landing-carousel-section').width()-250;
     parentWidth = parentWidth/6;

     gridster = $(".gridster").gridster({
         widget_base_dimensions: [parentWidth, parentWidth],
         max_cols: 6,
         max_rows: 6,
         widget_margins: [30, 30],
         shift_widgets_up: false,
         resize: {
             enabled: false
         }
     }).data('gridster').disable();

     $('.gridster').css({'padding': '30'});
 }
</script>
    </div>
                	</div>
</div>

