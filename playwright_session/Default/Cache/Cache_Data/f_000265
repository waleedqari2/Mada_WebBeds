            });

            var ShowCurrencyDialog = function(dataHref){
                var dialogOptions = {
                    size: 'large',
                    className: 'default_language_popup',
                    message: trans('changeCurrency.modalContent'),
                    title: trans('changeCurrency.title'),
                    onEscape: function() {},
                        buttons: {
                            close: {
                                label: trans('lang.button.no'),
                                className: 'btn-link btn-small'
                            },
                            success: {
                            label: trans('lang.button.yes'),
                                className: 'search_btn',
                                callback: function() {
                                  Compare.removeLocalStorage('localStorageObject');
                                  location.href = dataHref;
                                  return true;
                                }
                            }

                        }
                 };

                var langDialog = bootbox.dialog(dialogOptions);
            };
            /*checkbox click function for umrah*/
               $('#uhspecialCode').hide();
                $('#uhsubPCC').hide();
                $('#subpccCheckbox').click(function () {
                    if ($(this).is(":checked")) {
                        $('#uhsubPCC').show();
                        $('#uhspecialCode').show();
                    } else {
                        $('[name="subPcc"]').val('');
                        $('[name="subPcc_text"]').val('');
                        $('#uhsubPCC').hide();
                        $('#uhspecialCode').hide();
                    }
            });

            /*checkbox click method ends*/

			/* contact us methd */
			$('.contact_us_link').on('click', function (e) {
				e.preventDefault();
