# Copy to .env and edit values
CHROMEDRIVER_PATH=             # Optional: full path to chromedriver executable. If empty, webdriver-manager will be used.
HEADLESS=true                  # true/false to run Chrome headless
LOG_LEVEL=INFO
OUTPUT_DIR=./output
COOKIE_FILE=./cookies.json     # path where you will store uploaded cookies
UVICORN_HOST=127.0.0.1
UVICORN_PORT=8000
WORKERS=1
REQUEST_TIMEOUT=30
RETRY_ATTEMPTS=3
RETRY_BACKOFF=2                # seconds (base)